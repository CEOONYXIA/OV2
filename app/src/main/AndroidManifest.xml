        <activity android:name=".About" />
        <activity android:name=".Appointment" />
        <activity android:name=".JobList" />
        <activity android:name=".JobDetail" />
        <activity android:name=".ReviewList" />
        <activity android:name=".SaleList" />
        <activity android:name=".SaleDetail" />
        <activity android:name=".WorkDetail" />
        <activity android:name=".ReviewDetail" />
        <activity android:name=".WorkList"></activity>
    </application>

</manifest>